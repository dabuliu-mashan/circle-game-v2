{
    "version": 2,
    "framework": "python",
    "functions": {
        "api/index.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "FLASK_ENV": "production",
        "FLASK_APP": "api/index.py"
    }
}